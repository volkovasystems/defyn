  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystems/defyn/issues"
  },
  "homepage": "https://github.com/volkovasystems/defyn#readme",
  "dependencies": {
    "allkey": "^0.4.0",
    "anykey": "^0.4.0",
    "asyum": "^0.8.0",
    "babel-runtime": "^6.23.0",
    "dscrb": "^0.6.0",
    "falzy": "^0.20.0",
    "harden": "^0.22.0",
    "kein": "^0.28.0",
