{
  "name": "defyn",
  "version": "0.7.0",
  "description": "Define property descriptor.",
  "main": "defyn.js",
  "support": "defyn.support.js",
  "deploy": "defyn.deploy.js",
  "browser": "defyn.deploy.js",
  "scripts": {
    "build:definition": "babel definition.module.js --out-file definition.js --source-maps",
