{
	"name": "defyn",
	"version": "0.8.0",
	"description": "Define property descriptor.",
	"main": "defyn.js",
	"support": "defyn.support.js",
	"deploy": "defyn.deploy.js",
	"browser": "defyn.deploy.js",
	"scripts": {
		"pristine": "rsetmod clean && flxc execute ./.install.sh",
		"clear": "rsetmod clean --clear",
		"build:definition": "babel definition.module.js --out-file definition.js --source-maps",
		"build:defyn": "babel defyn.js --out-file defyn.support.js --source-maps",
		"build": "npm run build:definition && npm run build:defyn && webpack --progress --display-modules",
		"pretest": "npm run clear && npm run build",
		"test": "node defyn-test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/volkovasystems/defyn.git"
	},
	"keywords": [
		"define",
		"property",
		"descriptor",
		"defyn"
	],
	"author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
	"contributors": [
		"John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>",
		"Vinse Vinalon <vinsevinalon@gmail.com>"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkovasystems/defyn/issues"
	},
	"homepage": "https://github.com/volkovasystems/defyn#readme",
	"dependencies": {
		"allkey": "^0.5.0",
		"anykey": "^0.5.0",
		"asyum": "^0.9.0",
		"babel-runtime": "^6.25.0",
		"dscrb": "^0.7.0",
		"falzy": "^0.23.0",
		"harden": "^0.23.0",
		"kein": "^0.30.0",
		"protype": "^0.24.0",
		"truly": "^0.20.0",
		"zelf": "^0.14.0"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-latest": "^6.24.1",
		"source-map-loader": "^0.2.1",
		"webpack": "^3.5.2"
	}
}
